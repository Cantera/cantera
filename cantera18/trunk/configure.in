if test "$res" = "0" ; then
  tsc=tscompare_sh
else
  ./tscompare_csh s2 s1 2> /dev/null
  res=$?
  if test "$res" = "0" ; then
    tsc=tscompare_csh
  else
    ./tscompare_ksh s2 s1 2> /dev/null
    res=$?
    if test "$res" = "0" ; then
      tsc=tsccompare_ksh
    fi 
  fi

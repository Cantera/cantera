  BUILD_F90=0
fi

BUILD_CLIB=1

#----------------------------------------------------------------
#     Python Interface
#----------------------------------------------------------------

BUILD_PYTHON=0
if test "x$PYTHON_PACKAGE" = "xfull"; then
   BUILD_PYTHON=2
elif test "x$PYTHON_PACKAGE" = "xdefault"; then
   BUILD_PYTHON=2
elif test "x$PYTHON_PACKAGE" = "xminimal"; then
   BUILD_PYTHON=1

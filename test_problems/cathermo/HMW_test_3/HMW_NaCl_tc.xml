                <thetaCation cation1="Na+" cation2="H+">
                  <theta> 0.036 </theta>
                </thetaCation>

                <psiCommonAnion anion="Cl-" cation1="Na+" cation2="H+">
                  <Psi> -0.004 </Psi>
                </psiCommonAnion>

       </activityCoefficients>
    </thermo>
    <elementArray datasrc="elements.xml"> O H C E Fe Si N Na Cl </elementArray>
